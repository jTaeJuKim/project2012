
<% songs = Song.all.collect {|s| [ s.title, s.artist, s.key ] }  %>
<% songs.sort! %>

<table class= "table table-striped table-bordered">
	  	<thead>
	  		<th>Title</th>
	  		<th>Artist</th>
	  		<th>Root Key</th>
	  		<th>Add to Set</th>
	  	</thead>
	  	<tbody>
	  	  <% songs.each do |n| %>
	  	    <tr>
	  	      <td><%= songs.title %></td>
	  	      <td><%= songs.artist %></td>
	  	      <td><%= songs.key %></td>
	  	      <td><%= link_to "Add Song", '#' %></td>
	  	    </tr>
	  	  <% end %>
	  </tbody>
	  </table>