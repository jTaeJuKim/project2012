<h2>Create a new tag</h2>


<div class="span6 offset3">
	<%= form_for @category do |f|%>
	<%= render 'shared/error_messages', object: f.object %>
	  <%= f.label :tag, "Tag Description" %>
	  <%= f.text_field :tag %>

	  <%= f.submit "Create Tag", class: "btn btn-large btn-primary" %>
	<% end %>
</div>

