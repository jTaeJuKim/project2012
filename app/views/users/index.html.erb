<% provide(:title, 'All users') %>
<h1>BAND MEMBERS</h1>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Roles</th>
      <th>Click to delete</th>
    </tr>
  </thead>
  <tbody>
    <%= render @users %>
  </tbody>
</table>

<%= link_to "Add Band Member", new_user_path, class: "btn btn-large btn-primary" %>
