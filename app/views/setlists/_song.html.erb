<div id="songSelection">
<table class= "table table-striped table-bordered">
	  	<thead>
	  		<th>Title</th>
	  		<th>Artist</th>
	  		<th>Root Key</th>
	  		<th>Add to Set</th>
	  	</thead>
	  	<tbody>
	  	  <% @songs.each do |song| %>
            <tr>
               <td><%= song.title %></td>
               <td><%= song.artist %></td>
               <td><%= song.key %></td>
               <td><%= link_to "Add to Set", '#' %></td>
            </tr>
          <% end %>
	    </tbody>
</table>
</div>