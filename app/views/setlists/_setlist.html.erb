<tr>
	<td><%= link_to setlist.date, setlist_path(setlist) %></td>
	<td>
		<% if(setlist.morning == true) %>
		  <%= "Morning Service" %>
		<% else %>
		  <%= "Evening Service" %>
		<% end %>
	</td>

	<% if current_user.admin? %>
	
	<td>
		<%= link_to "Add/Remove Songs", edit_setlist_path(setlist) %>
	</td>
	<td><%= link_to "Delete", setlist, method: :delete,
	                                data: { confirm: 
	                                	"Are you sure you want to delete this set?" }%>
	</td>
	
	<% end %>

</tr>