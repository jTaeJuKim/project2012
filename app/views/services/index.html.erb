<h3>Current Rota</h3>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Date</th>
      <th>Leader</th>
      <th>Acoustic</th>
      <th>Electric</th>
      <th>Piano</th>
      <th>Bass</th>
      <th>Drums</th>
      <th>Sax</th>
      <th>Violin</th>
      <th>Singers</th>
      <th>AM Sound</th>
      <th>PM Sound</th>
      <% if current_user.admin? %>
      <th>Delete</th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <%= render @services %>
  </tbody>
</table>


<div>
<% if current_user.admin? %>
   <%= link_to "Plan Another Service", new_service_path, class: "btn btn-large btn-primary" %>
<% end %>
</div>


<div id="singerBox">
  <ul>
    <li>
      Vox 1: <% @vox1.users.each do |n| %>
      <%= n.name + " " + n.surname + " | " %>
      <% end %>
    </li>
    <li>
      Vox 2: <% @vox2.users.each do |n| %>
      <%= n.name + " " + n.surname + " | " %>
      <% end %>
    </li>
    <li>
      Vox 3: <% @vox3.users.each do |n| %>
      <%= n.name + " " + n.surname + " | " %>
      <% end %>
    </li>
  </ul>
</div>